{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [
    ".env"
  ],
  "pipeline": {
    "build": {
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {},
    "start": {},
    "lint": {},
    "deploy": {
      "dotEnv": [
        ".env"
      ]
    },
    "@wba/iac#deploy": {
      "dotEnv": [
        ".env"
      ]
    },
    "lint:fix": {},
    "type-check": {},
    "deploy:ci": {
      "dependsOn": ["^build"]
    },
    "@wba/scripts#start": {
      "dependsOn": [
        "@wba/logic#build"
      ]
    }
  }
}
